{
	"tabela": {
		"nome": "PCINTEGRACAOROTASERVICO",
		"campos": [
			{
				"nome": "SOMENTEATUALIZARINTEGRACAOCORE",
				"valor": "N"
			},
			{
				"nome": "ID",
				"valor": "PDVSYNC - Consultar retorno lote externo"
			},
			{
				"nome": "IDEMPRESAAPI",
				"valor": "PDVSYNC"
			},
			{
				"nome": "SERVICO",
				"valor": "PDVSYNC - Consultar retorno lote externo"
			},
			{
				"nome": "LAYOUTCOMUNICACAO",
				"valor": {
					"name": "PDVSYNC - Consultar retorno lote externo",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{TOKEN}}"
							},
							{
								"key": "Accept",
								"value": "*/*"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"url": {
							"raw": "{{BASE_URL_STATUS_LOTE}}/{{ID_INQUILINO}}/{{LOTE_ORIGEM}}",
							"query": []
						}
					},
					"response": []
				}
			},
			{
				"nome": "LAYOUTTRANSFORMACAO",
				"valor": [
					{
						"operation": "shift",
						"spec": {
							"data": {
								"*": {
									"loteOrigem": [
										"data[&1].numeroLote",
										"data[&1].idExterno"
									],
									"status": [
										"data[&1].status",
										"data[&1].statusIdExterno"
									],
									"errosIdentificados": "data[&1].errosIdentificados"
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"data": {
								"*": {
									"numeroLote": "items[&1].numeroLote",
									"idExterno": "items[&1].idExterno",
									"statusIdExterno": "items[&1].statusIdExterno",
									"status": {
										"1": {
											"#11": "items[&3].status"
										},
										"1.0": {
											"#11": "items[&3].status"
										},
										"2": {
											"#11": "items[&3].status"
										},
										"2.0": {
											"#11": "items[&3].status"
										},
										"3": {
											"#13": "items[&3].status"
										},
										"3.0": {
											"#13": "items[&3].status"
										},
										"*": {
											"#99": "items[&3].origem"
										}
									},
									"errosIdentificados": {
										"*": {
											"idRetaguarda": "items[&3].erros[&1].idInterno",
											"erro": "items[&3].erros[&1].erro"
										}
									}
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"idExterno": "=concat('pdvsync-post-status-', @(1,statusIdExterno),'-lote-externo-', @(1,idExterno))"
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"numeroLote": "items[&1].numeroLote",
									"idExterno": "idExterno",
									"status": "items[&1].filiais[0].status",
									"erros": "items[&1].filiais[0].erros"
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"filiais": {
										"*": {
											"status": [
												"=toInteger",
												0
											]
										}
									}
								}
							}
						}
					},
					{
						"operation": "modify-default-beta",
						"spec": {
							"items": {
								"*": {
									"aceitaBaixaParcial": false
								}
							}
						}
					},
					{
						"operation": "default",
						"spec": {
							"_attr_access": "items"
						}
					}
				]
			},
			{
				"nome": "ATIVO",
				"valor": "N"
			},
			{
				"nome": "AUTENTICADOR",
				"valor": "N"
			},
			{
				"nome": "DATASINCRONISMO",
				"valor": "17-MAR-23"
			},
			{
				"nome": "REFRESHTOKEN",
				"valor": ""
			}
		]
	}
}