{
	"tabela": {
		"nome": "PCINTEGRACAOROTASERVICO",
		"campos": [
			{
				"nome": "SOMENTEATUALIZARINTEGRACAOCORE",
				"valor": "N"
			},
			{
				"nome": "ID",
				"valor": "WTA - Buscar Filtro NCM Tributo"
			},
			{
				"nome": "IDEMPRESAAPI",
				"valor": "WINTHOR-WTA"
			},
			{
				"nome": "SERVICO",
				"valor": "WTA - Buscar Filtro NCM Tributo"
			},
			{
				"nome": "LAYOUTCOMUNICACAO",
				"valor": {
					"name": "WTA - Buscar Filtro NCM Tributo",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{TOKEN}}"
							},
							{
								"key": "Accept",
								"value": "*/*"
							}
						],
						"url": {
							"raw": "{{URL_BASE}}/winthor/tributacao/v0/saida/tributacao/excecao/consultar/pdvsync",
							"query": [
								{
									"key": "pageSize",
									"value": "{{PAGE_SIZE}}"
								},
								{
									"key": "page",
									"value": "{{PAGE}}"
								},
								{
									"key": "lastChange",
									"value": "{{LAST_CHANGE}}"
								},
								{
									"key": "branchId",
									"value": "{{BRANCHID}}"
								},
								{
									"key": "exceptionType",
									"value": "{{EXCEPTION_TYPE}}"
								}
							]
						}
					},
					"response": []
				}
			},
			{
				"nome": "LAYOUTTRANSFORMACAO",
				"valor": [
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"codigoTributacao": "items[&1].codigoTributacao",
									"ncm": "items[&1].ncm",
									"dataUltimaAlteracao": "items[&1].dataUltimaAlteracao",
									"tiposDetalheImposto": {
										"*": "items[&2].tiposDetalheImpostoTmp[].tipoImposto"
									}
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"tiposDetalheImpostoTmp": {
										"*": {
											"idExterno": "=concat('pdvsync-filtro-ncm-tributo-', @(3,codigoTributacao),'-',@(1,tipoImposto),'-',@(3,ncm),'-',@(3,dataUltimaAlteracao))",
											"tipoIdInterno": "PDVSYNC-FILTRO-NCM-TRIBUTO",
											"idInterno": "=concat(@(3,codigoTributacao),'-',@(1,tipoImposto),'-',@(3,ncm))",
											"idRetaguarda": "=concat(@(3,codigoTributacao),'-',@(1,tipoImposto),'-',@(3,ncm))",
											"idRetaguardaImpostoFiscal": "=concat(@(3,codigoTributacao),'-',@(1,tipoImposto))",
											"idRetaguardaNcm": "=concat('',@(3,ncm))"
										}
									}
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"tiposDetalheImpostoTmp": {
										"*": "items[]"
									}
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"idInquilino": "{{ID_INQUILINO}}",
									"idProprietario": "{{MASTER_ID_PROPRIETARIO}}",
									"loteOrigem": "{{LOTE_ORIGEM}}"
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"@": "items[]",
									"idExterno": "idExterno[]",
									"idInterno": "idInterno[]",
									"tipoIdInterno": "tipoIdInterno[]"
								}
							}
						}
					},
					{
						"operation": "default",
						"spec": {
							"_attr_access": "items"
						}
					}
				]
			},
			{
				"nome": "ATIVO",
				"valor": "S"
			},
			{
				"nome": "AUTENTICADOR",
				"valor": "N"
			},
			{
				"nome": "DATASINCRONISMO",
				"valor": "14-NOV-23"
			},
			{
				"nome": "REFRESHTOKEN",
				"valor": ""
			},
			{
				"nome": "TIPOPROCESSO",
				"valor": "BUSCAR"
			}
		]
	}
}