{
  "tabela": {
    "nome": "PCINTEGRACAOROTASERVICO",
    "campos": [
      {
        "nome": "SOMENTEATUALIZARINTEGRACAOCORE",
        "valor": "N"
      },
      {
        "nome": "ID",
        "valor": "WTA - Buscar Grupo Campanha Cliente"
      },
      {
        "nome": "IDEMPRESAAPI",
        "valor": "WINTHOR-WTA"
      },
      {
        "nome": "SERVICO",
        "valor": "WTA - Buscar Grupo Campanha Cliente"
      },
      {
        "nome": "LAYOUTCOMUNICACAO",
        "valor": {
          "name": "WTA -Buscar Grupo Campanha Cliente",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{TOKEN}}"
              },
              {
                "key": "Accept",
                "value": "*/*"
              }
            ],
            "url": {
              "raw": "{{URL_BASE}}/winthor/cliente/v1/grupoCampanha",
              "query": [
                {
                  "key": "pageSize",
                  "value": "{{PAGE_SIZE}}"
                },
                {
                  "key": "page",
                  "value": "{{PAGE}}"
                },
                {
                  "key": "lastChange",
                  "value": "{{DATAALTERACAO}}"
                }
              ]
            }
          },
          "response": []
        }
      },
      {
        "nome": "LAYOUTTRANSFORMACAO",
        "valor": [
          {
            "operation": "modify-overwrite-beta",
            "spec": {
              "items": {
                "*": {
                  "dadosTransformados": "=base64ToObject"
                }
              }
            }
          },
          {
            "operation": "modify-overwrite-beta",
            "spec": {
              "items": {
                "*": {
                  "idExterno": "=concat('pdvsync-grupo-campanha-cliente-', @(1,lastChange))",
                  "tipoIdInterno": "PDVSYNC-GRUPO-CAMPANHA-CLIENTE"
                }
              }
            }
          },
          {
            "operation": "shift",
            "spec": {
              "items": {
                "*": {
                  "idExterno": "idExterno",
                  "groupCampaignItems": {
                    "*": {
                      "itemId": "items.[&3].listRetaguarda[&1].idRetaguardaCliente",
                      "@(2,branchId)": "items.[&3].listRetaguarda[&1].idRetaguardaLoja",
                      "@(2,groupType)": "items.[&3].listRetaguarda[&1].idRetaguarda",
                      "@(2,active)": "items.[&3].listRetaguarda[&1].SituacaoItem"
                    }
                  }
                }
              }
            }
          },
          {
            "operation": "shift",
            "spec": {
              "idExterno": "idExterno",
              "items": {
                "*": {
                  "listRetaguarda": {
                    "*": "items"
                  }
                }
              }
            }
          },
          {
            "operation": "shift",
            "spec": {
              "idExterno": "idExterno",
              "items": {
                "*": {
                  "idRetaguardaCliente": "items.[&1].idRetaguardaCliente",
                  "idRetaguardaLoja": "items.[&1].idRetaguardaLoja",
                  "idRetaguarda": "items.[&1].idRetaguarda",
                  "SituacaoItem": {
                    "true": {
                      "#1": "items.[&3].SituacaoItem"
                    },
                    "false": {
                      "#0": "items.[&3].SituacaoItem"
                    }
                  }
                }
              }
            }
          },
          {
            "operation": "default",
            "spec": {
              "_attr_access": "items",
              "items[]": {
                "*": {
                  "idInquilino": "{{ID_INQUILINO}}",
                  "idProprietario": "{{MASTER_ID_PROPRIETARIO}}",
                  "loteOrigem": "{{LOTE_ORIGEM}}"
                }
              }
            }
          }
        ]
      },
      {
        "nome": "ATIVO",
        "valor": "N"
      },
      {
        "nome": "AUTENTICADOR",
        "valor": "N"
      },
      {
        "nome": "DATASINCRONISMO",
        "valor": "14-NOV-23"
      },
      {
        "nome": "REFRESHTOKEN",
        "valor": ""
      },
      {
        "nome": "TIPOPROCESSO",
        "valor": "BUSCAR"
      }
    ]
  }
}