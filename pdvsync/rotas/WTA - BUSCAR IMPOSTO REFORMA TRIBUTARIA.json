{
	"tabela": {
		"nome": "PCINTEGRACAOROTASERVICO",
		"campos": [
			{
				"nome": "SOMENTEATUALIZARINTEGRACAOCORE",
				"valor": "N"
			},
			{
				"nome": "ID",
				"valor": "WTA - Buscar Imposto Reforma Tributaria"
			},
			{
				"nome": "IDEMPRESAAPI",
				"valor": "WINTHOR-WTA"
			},
			{
				"nome": "SERVICO",
				"valor": "WTA - Buscar Imposto Reforma Tributaria"
			},
			{
				"nome": "LAYOUTCOMUNICACAO",
				"valor": {
					"name": "WTA - Buscar Imposto Reforma Tributaria",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{TOKEN}}"
							},
							{
								"key": "Accept",
								"value": "*/*"
							}
						],
						"url": {
							"raw": "{{URL_BASE}}/winthor/tributacao/v0/saida/tributacao/consultar/pdvsync",
							"query": [
								{
									"key": "pageSize",
									"value": "{{PAGE_SIZE}}"
								},
								{
									"key": "page",
									"value": "{{PAGE}}"
								},
								{
									"key": "lastChange",
									"value": "{{LAST_CHANGE}}"
								},
								{
									"key": "taxType",
									"value": "{{TAX_TYPE}}"
								},
								{
									"key": "branchId",
									"value": "{{BRANCHID}}"
								}
							]
						}
					},
					"response": []
				}
			},
			{
				"nome": "LAYOUTTRANSFORMACAO",
				"valor": [
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"idExterno": "=concat('pdvsync-imposto-rt-',@(1,codigoTributacao),'-',@(1,dataUltimaAlteracao))",
									"idInterno": "=concat('', @(1,codigoTributacao))",
									"tipoIdInterno": "PDVSYNC-IMPOSTO-RT"
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"idExterno": "idExterno[]",
									"idInterno": "idInterno",
									"tipoIdInterno": "tipoIdInterno",
									"codigoTributacao": "items.[&1].idRetaguarda",
									"ativo": {
										"true": {
										"#1": "items.[&3].situacao"
										},
										"*": {
										"#0": "items.[&3].situacao"
										}
									},
									"tipoImposto": "items.[&1].tipoImposto",
									"cst": "items.[&1].cst",
									"descricaoTributacao": "items.[&1].descricaoCst",
									"classificacaoTributaria": "items.[&1].cclasstrib",
									"valorAliquota": "items.[&1].percentualAliquota",
									"dataInicioVigencia": "items.[&1].dataInicioVigencia",
									"dataFimVigencia": "items.[&1].dataFimVigencia"
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"idRetaguarda": "=toString",
									"situacao": "=toInteger",
									"cclasstrib": "=toInteger",
									"percentualAliquota": "=toDouble"
								}
							}
						}
					},
					{
						"operation": "default",
						"spec": {
							"_attr_access": "items",
							"items[]": {
								"*": {
									"idInquilino": "{{ID_INQUILINO}}",
									"idProprietario": "{{MASTER_ID_PROPRIETARIO}}",
									"loteOrigem": "{{LOTE_ORIGEM}}"
								}
							}
						}
					}	
				]
			},
			{
				"nome": "ATIVO",
				"valor": "S"
			},
			{
				"nome": "AUTENTICADOR",
				"valor": "N"
			},
			{
				"nome": "DATASINCRONISMO",
				"valor": "14-NOV-23"
			},
			{
				"nome": "REFRESHTOKEN",
				"valor": ""
			},
			{
				"nome": "TIPOPROCESSO",
				"valor": "BUSCAR"
			}
		]
	}
}