{
	"tabela": {
		"nome": "PCINTEGRACAOROTASERVICO",
		"campos": [
			{
				"nome": "SOMENTEATUALIZARINTEGRACAOCORE",
				"valor": "N"
			},
			{
				"nome": "ID",
				"valor": "WTA - Buscar Imposto Reforma Tributaria"
			},
			{
				"nome": "IDEMPRESAAPI",
				"valor": "WINTHOR-WTA"
			},
			{
				"nome": "SERVICO",
				"valor": "WTA - Buscar Imposto Reforma Tributaria"
			},
			{
				"nome": "LAYOUTCOMUNICACAO",
				"valor": {
					"name": "WTA - Buscar Imposto Reforma Tributaria",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{TOKEN}}"
							},
							{
								"key": "Accept",
								"value": "*/*"
							}
						],
						"url": {
							"raw": "{{URL_BASE}}/winthor/tributacao/v0/saida/tributacao/consultar/pdvsync",
							"query": [
								{
									"key": "pageSize",
									"value": "{{PAGE_SIZE}}"
								},
								{
									"key": "page",
									"value": "{{PAGE}}"
								},
								{
									"key": "lastChange",
									"value": "{{LAST_CHANGE}}"
								},
								{
									"key": "taxType",
									"value": "{{TAX_TYPE}}"
								},
								{
									"key": "branchId",
									"value": "{{BRANCHID}}"
								}
							]
						}
					},
					"response": []
				}
			},
			{
				"nome": "LAYOUTTRANSFORMACAO",
				"valor": [
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"codigoTributacao": "items[&1].codigoTributacao",
									"cst": "items[&1].cst",
									"descricaoTributacao": "items.[&1].descricaoCst",
									"classificacaoTributaria": "items.[&1].cclasstrib",
									"ativo": {
										"true": {
											"#1": "items.[&3].situacao"
										},
										"*": {
											"#0": "items.[&3].situacao"
										}
									},
									"dataInicioVigencia": "items.[&1].dataInicioVigencia",
									"dataFimVigencia": "items.[&1].dataFimVigencia",
									"dataUltimaAlteracao": "items.[&1].dataUltimaAlteracao",
									"impostos": {
										"*": {
											"tipoDetalheImposto": "items[&3].impostosTmp[&1].tipoImposto",
											"percentualAliquota": "items[&3].impostosTmp[&1].percentualAliquota",
											"percentualReducaoAliquota": "items[&3].impostosTmp[&1].percentualReducaoAliquota"
										}
									}
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"impostosTmp": {
										"*": {
											"idRetaguarda": "=concat(@(3,codigoTributacao),'-',@(1,tipoImposto))",
											"idExterno": "=concat('pdvsync-imposto-rt-',@(3,codigoTributacao),'-',@(1,tipoImposto),'-',@(3,dataUltimaAlteracao))",
											"tipoIdInterno": "PDVSYNC-IMPOSTO-RT",
											"idInterno": "=concat(@(3,codigoTributacao),'-',@(1,tipoImposto))",
											"cst": "=concat('', @(3,cst))",
											"descricaoCst": "=concat('', @(3,descricaoCst))",
											"cclasstrib": "=concat('', @(3,cclasstrib))",
											"situacao": "=concat('', @(3,situacao))",
											"dataInicioVigencia": "=concat('', @(3,dataInicioVigencia))",
											"dataFimVigencia": "=concat('', @(3,dataFimVigencia))"
										}
									}
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"impostosTmp": {
										"*": "items[]"
									}
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"idInquilino": "{{ID_INQUILINO}}",
									"idProprietario": "{{MASTER_ID_PROPRIETARIO}}",
									"loteOrigem": "{{LOTE_ORIGEM}}"
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"@": "items[]",
									"idExterno": "idExterno[]",
									"idInterno": "idInterno[]",
									"tipoIdInterno": "tipoIdInterno[]"
								}
							}
						}
					},
					{
						"operation": "default",
						"spec": {
							"_attr_access": "items"
						}
					}
				]
			},
			{
				"nome": "ATIVO",
				"valor": "S"
			},
			{
				"nome": "AUTENTICADOR",
				"valor": "N"
			},
			{
				"nome": "DATASINCRONISMO",
				"valor": "14-NOV-23"
			},
			{
				"nome": "REFRESHTOKEN",
				"valor": ""
			},
			{
				"nome": "TIPOPROCESSO",
				"valor": "BUSCAR"
			}
		]
	}
}