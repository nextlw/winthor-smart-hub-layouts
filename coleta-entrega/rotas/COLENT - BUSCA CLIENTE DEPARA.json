{
    "tabela": {
        "nome": "PCINTEGRACAOROTASERVICO",
        "campos": [
            {
                "nome": "DATASINCRONISMO",
                "valor": ""
            },
            {
                "nome": "REFRESHTOKEN",
                "valor": "N"
            },
            {
                "nome": "SOMENTEATUALIZARINTEGRACAOCORE",
                "valor": "N"
            },
            {
                "nome": "ID",
                "valor": "COLENT - Busca Cliente DePara"
            },
            {
                "nome": "IDEMPRESAAPI",
                "valor": "COLETA-ENTREGA"
            },
            {
                "nome": "SERVICO",
                "valor": "COLENT - Busca Cliente DePara"
            },
            {
                "nome": "LAYOUTCOMUNICACAO",
                "valor": {
                    "name": "COLETA-ENTREGA - Busca Cliente",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{TOKEN}}"
                            },
                            {
                                "key": "Accept",
                                "value": " */*"
                            },
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "url": {
                            "raw": "https://winthor-log.logistica.totvs.app/coletaentrega/query/api/v1/clientes",
                            "query": [
                                {
                                    "key": "documentoIdentificacao",
                                    "value": "{{DOCUMENTO_IDENTIFICACAO}}"
                                }
                            ]
                        }
                    },
                    "response": []
                }
            },
            {
                "nome": "LAYOUTTRANSFORMACAO",
                "valor": [
                    {
                        "operation": "modify-overwrite-beta",
                        "spec": {
                            "items": {
                                "*": {
                                    "idExterno": "\u003dconcat(\u0027teste-depara-gravacao-\u0027,@(1,documentoIdentificacao),\u0027-\u0027,@(1,id))"
                                }
                            }
                        }
                    },
                    {
                        "operation": "shift",
                        "spec": {
                            "items": {
                                "*": {
                                    "idExterno": "idExterno",
                                    "id": "items.[0].idexterno",
                                    "documentoIdentificacao": "items.[0].idinterno"
                                }
                            }
                        }
                    },
                    {
                        "operation": "default",
                        "spec": {
                            "items[]": {
                                "*": {
                                    "campo": "CGCENT",
                                    "tabela": "PCCLIENT"
                                }
                            },
                            "_attr_access": "items"
                        }
                    }
                ]
            },
            {
                "nome": "ATIVO",
                "valor": "S"
            },
            {
                "nome": "TIPOPROCESSO",
                "valor": ""
            },
            {
                "nome": "AUTENTICADOR",
                "valor": "N"
            }
        ]
    }
}